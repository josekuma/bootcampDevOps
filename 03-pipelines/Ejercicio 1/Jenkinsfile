pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps{
checkout scmGit(branches: [[name: '*/master']], extensions: [[$class: 'RelativeTargetDirectory', relativeTargetDir: '/03-pipelines/Ejercicio 1/jenkins-resources/']], userRemoteConfigs: [[url: '\'https://github.com/josekuma/bootcampDevOps.git\'']]) 


         
            }

        }
        stage('Compile') {
            steps {
                sh './gradlew compileJava'
            }
        }
        stage('Unit Tests') {
            steps {
                sh './gradlew test'
            }
        }
    }
}
